{
  "name": "PackageUpdater",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf dist",
    "build": "electron-packager . --out=dist",
    "setup": "electron-installer-windows --src dist/electron-quick-start-win32-x64/ --dest dist/installers/ --config config.json ",
    "final": "npm run clean && npm run build && npm run setup"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "IlyaShishov",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-installer-windows": "^0.2.0",
    "electron-wix-msi": "^1.2.3",
    "electron-builder": "^19.49.0",
    "electron-prebuilt": "^1.4.13",
    "electron-winstaller": "^2.6.3"
  },
  "dependencies": {
    "react": "^16.2.0"
  }
}
